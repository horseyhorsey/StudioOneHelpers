@page "/stickers"
@using Blazored.LocalStorage
@using StudioOneHelpers.Pages.Shared
@inject Blazored.LocalStorage.ILocalStorageService localStorage

<PageTitle>Controller Layout Designer - S1 Helpers</PageTitle>

<MudContainer Class="mt-2 px-8" MaxWidth="MaxWidth.False">
    <MudGrid Justify="Justify.Center">
        <MudItem xs="12">
            <CascadingValue Value="localStorage">
                <StickersComponent />
            </CascadingValue>
        </MudItem>
    </MudGrid>
</MudContainer>


